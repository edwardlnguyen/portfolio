<% provide(:title, 'find stock') %>
<h1>Find stock</h1>
<div class="row">
  <div class="col-md-6 col-md-offset-3">
    <%= form_for(@stock) do |f| %>

      <%= f.label :symbol %>
      <%= f.text_field :symbol, class: 'form-control' %>

      <%= f.submit "Find Stock", class: "btn btn-primary" %>
    <% end %>
  </div>
</div>